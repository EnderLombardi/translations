<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">

	<Dialog id="disruptionDetailPopUp" class="classDisruptionDetailPopUp"
		contentWidth="90%" contentHeight="90%" verticalScrolling="true"
		showHeader="false" afterClose="afterCloseDisruptnDetailPopUp">
		<content>
			<NavContainer id="disruptDetailNavContainer" autoFocus="false"
				afterNavigate="afterNavigate" navigate="onNavigate">
				<pages>

				</pages>
			</NavContainer>

		</content>
		<buttons>
			<Button id="btnUpdateDisruption" text="{disruptiontrackerI18n>update}"
				type="Transparent" icon="sap-icon://save" press="onUpdateDisruption" visible="false" />=
			<Button id="btnCancelDisruption" text="{disruptiontrackerI18n>cancel}"
				type="Transparent" icon="sap-icon://sys-cancel" press="onCancelCreateDisruption" visible="false" />

			<Button id="btnCloseDisruptionPopup" text="{disruptiontrackerI18n>Close}" icon="sap-icon://decline"
				press="onCloseDisruptnDetailPopUp" />
		</buttons>
	</Dialog>

</core:FragmentDefinition>
