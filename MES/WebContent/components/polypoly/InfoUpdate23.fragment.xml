<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified" xmlns:table="sap.ui.table">
	<Dialog id="infoUpdate23" stretchOnPhone="true" state="Warning" title="Warning">
		<buttons>
			<Button text="Continue" press="onContinueInfoUpdate23" />
			<Button text="Cancel" press="onCancelInfoUpdate23" />
		</buttons>
		<content>
			<VBox alignItems="Center">
				<items>
					<Label text="Operator {/name} doesn't have following QA:" textAlign="Center" />
					<Table items="{path: '/Row'}">
						<columns>
							<Column hAlign="Center" width="10rem"/>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{QA}" />
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</items>
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>