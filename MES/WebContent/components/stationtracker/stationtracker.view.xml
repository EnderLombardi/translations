<core:View xmlns:core="sap.ui.core" xmlns="sap.m"
	xmlns:suite="sap.suite.ui.commons" controllerName="airbus.mes.stationtracker.stationtracker"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:custom="airbus.mes.stationtracker" xmlns:control="airbus.mes.stationtracker.control"
	xmlns:layout="sap.ui.layout" xmlns:micro="sap.suite.ui.microchart">
	<Page title="Station Tracker" height="100%" showHeader="false"
		enableScrolling="false " class="GenricBgColor">
			
		<content width="100%" height="100%">
			<Panel id="kpi_header" expandable="true" expanded="true" expandAnimation="false"
				class="pageHeader sapUiSmallMarginTop contentNoPad">
				<headerToolbar>
					<Toolbar id="kpiHeaderToolbar">
						<DatePicker id="DatePicker" width="auto" class="pageHeaderBlue" />
						<Select id="selectShift"
							class="sapUiTinyMarginBeginEnd pageHeader pageHeaderBlue" change="changeShift"
							items="{ path : 'stationTrackerShift>/' }"
							enabled="{ path : 'stationTrackerShift>/0/visible', formatter : 'airbus.mes.stationtracker.util.Formatter.isDayTimeline' }">
							<core:Item key="{stationTrackerShift>id}" text="{stationTrackerShift>value}" />
						</Select>
						<Button id="ProductionButton" text="{StationTrackerI18n>ProductionGroup}" type="Transparent"
							class="whiteColor whiteButton" press="onProductionGroupPress" />
						<Select id="selectUser" change="selectUser"
							class="sapUiTinyMarginBeginEnd pageHeader pageHeaderBlue" items="{ path : 'ressourcePoolModel>/Rowsets/Rowset/0/Row' }">
							<core:Item key="{ressourcePoolModel>user}" text="{ressourcePoolModel>user}" />
						</Select>

						<ToolbarSpacer />
						<Button id="TeamButton" text="{StationTrackerI18n>ButtonTeam}" type="Transparent"
							class="tiffanyColor whiteButton buttonStatus2 iconbuttonTeam sapUiSmallMarginBegin"
							icon="sap-icon://group" press="onTeamPress" />
					</Toolbar>

				</headerToolbar>
				<content>
					<HBox id="kpiHeaderContent" width="100%">

						<VBox id="boxTaktAdherence" width="35%" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>TaktAdherence}" design="Bold"
								width="100%" class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<HBox>
								<VBox width="60%">
									<micro:AreaMicroChart colorPalette='#ffffff , #84caec,'
										width="90%" height="5rem" minXValue="0" maxXValue="100"
										minYValue="0" maxYValue="100" class="sapUiSmallMargin" press="press">
										<micro:firstXLabel>
											<micro:AreaMicroChartLabel label="June 1"
												color="Good" />
										</micro:firstXLabel>
										<micro:lastXLabel>
											<micro:AreaMicroChartLabel label="June 30"
												color="Critical" />
										</micro:lastXLabel>
										<micro:firstYLabel>
											<micro:AreaMicroChartLabel label="0M"
												color="Good" />
										</micro:firstYLabel>
										<micro:lastYLabel>
											<micro:AreaMicroChartLabel label="80M"
												color="Critical" />
										</micro:lastYLabel>
										<micro:lines>
											<micro:AreaMicroChartItem>
												<micro:points>
													<micro:AreaMicroChartPoint x="80"
														y="0" />
													<micro:AreaMicroChartPoint x="80"
														y="100" />
												</micro:points>
											</micro:AreaMicroChartItem>

											<micro:AreaMicroChartItem>
												<micro:points>
													<micro:AreaMicroChartPoint x="0"
														y="0" />
													<micro:AreaMicroChartPoint x="10"
														y="30" />
													<micro:AreaMicroChartPoint x="60"
														y="40" />
													<micro:AreaMicroChartPoint x="100"
														y="90" />
												</micro:points>
											</micro:AreaMicroChartItem>

										</micro:lines>

										<micro:maxThreshold>
											<micro:AreaMicroChartItem color="Good">
												<micro:points>
													<micro:AreaMicroChartPoint x="0"
														y="0" />
													<micro:AreaMicroChartPoint x="30"
														y="50" />
													<micro:AreaMicroChartPoint x="60"
														y="80" />
													<micro:AreaMicroChartPoint x="100"
														y="100" />
												</micro:points>
											</micro:AreaMicroChartItem>
										</micro:maxThreshold>

										<micro:minThreshold>
											<micro:AreaMicroChartItem color="Error">
												<micro:points>
													<micro:AreaMicroChartPoint x="0"
														y="0" />
													<micro:AreaMicroChartPoint x="30"
														y="50" />
													<micro:AreaMicroChartPoint x="60"
														y="80" />
													<micro:AreaMicroChartPoint x="100"
														y="100" />
												</micro:points>
											</micro:AreaMicroChartItem>
										</micro:minThreshold>

									</micro:AreaMicroChart>
								</VBox>

								<VBox width="30%">
									<Label text="{StationTrackerI18n>Adherence}" class=" whiteText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/EfficiencyFactors/Labour}"
										class="sapUiSmallMarginTop kpiValue  whiteText"
										height="100%" />

								</VBox>

								<VBox width="30%">
									
									<layout:Grid
										class="sapUiSmallMarginTop"
										defaultSpan="L6 M6 S6">
										<layout:content>
											<Label text="{StationTrackerI18n>Trend}" class=" whiteText" />
											<core:Icon src="sap-icon://up" class="sapUiSmallMarginBeginEnd TrendGreen" />

											<Label text="{StationTrackerI18n>DeltaVsSLB}" class=" whiteText" />
											<Label text="{KPI>/Rowsets/Rowset/0/Row/EfficiencyFactors/SLB}" class="whiteText" />

											<Label text="{StationTrackerI18n>DeltaVsSoA}" class=" whiteText" />
											<Label text="{KPI>/Rowsets/Rowset/0/Row/EfficiencyFactors/SLB}" class="whiteText" />

											<Label text="{StationTrackerI18n>CycleTime}" class=" whiteText" />
											<Label text="{KPI>/Rowsets/Rowset/0/Row/EfficiencyFactors/SLB}" class="whiteText" />

											<Label text="{StationTrackerI18n>WorkCintent}" class=" whiteText" />
											<Label text="{KPI>/Rowsets/Rowset/0/Row/EfficiencyFactors/SLB}" class="whiteText" />
										</layout:content>
									</layout:Grid>

								</VBox>


							</HBox>

						</VBox>

						<VBox  id="boxLabourEfficiency" wrap="Wrap" width="5%" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>LabourEfficiency}" design="Bold"
								width="100%" class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<Label text="{KPI>/Rowsets/Rowset/0/Row/EfficiencyFactors/SLB}"
								class="sapUiSmallMarginTop whiteText kpiValue" />
						</VBox>

						<VBox id="boxSLBEfficiecy" width="10%" alignItems="Center" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>SLBEFFICIENCY}" design="Bold"
								class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<VBox width="90%">
								<micro:ComparisonMicroChart width="100%"
									size="L" colorPalette="#74d2e7, #fe5000, #fe5000">
									<micro:data>
										<micro:ComparisonMicroChartData
											title="{StationTrackerI18n>TaktSLB}" value="8" />
										<micro:ComparisonMicroChartData
											title="{StationTrackerI18n>WhiteSpaces}" value="3" />
										<micro:ComparisonMicroChartData
											title="{StationTrackerI18n>RedSpaces}" value="10" />
									</micro:data>
								</micro:ComparisonMicroChart>
							</VBox>
						</VBox>

						<VBox id="boxOpenDisruptions" width="25%" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>DisruptionOpen/Andon}" design="Bold"
								width="100%" class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<HBox>
								<VBox width="20%">
									<Label text="{StationTrackerI18n>DisruptionTotal}" class=" whiteText"
										design="Bold" />
									<Label text="{StationTrackerI18n>Disruption}" class=" whiteText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" whiteText kpiValue" design="Bold" />
									<Label text="{StationTrackerI18n>Andon}" class=" redText"
										design="Bold" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class="redText kpiValue" design="Bold" />
								</VBox>
								<VBox width="30%">
									<Label text="{StationTrackerI18n>DisruptionDetails}"
										class="disruptionText" design="Bold" />
									<Label text="{StationTrackerI18n>DisruptionsNC}" class="disruptionText" />
									<Label text="{StationTrackerI18n>DisruptionsMissing}"
										class="disruptionText" />
									<Label text="{StationTrackerI18n>DisruptionsTools}"
										class="disruptionText" />
									<Label text="{StationTrackerI18n>DisruptionFTE}" class="disruptionText" />
									<Label text="{StationTrackerI18n>DisruptionOther}" class="disruptionText" />
								</VBox>
								<VBox alignItems="Center" width="26%">
									<Label text="{StationTrackerI18n>Disruption}" class="whiteText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" whiteText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" whiteText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" whiteText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" whiteText" />
								</VBox>
								<VBox alignItems="Center" width="26%">
									<Label text="{StationTrackerI18n>Andon}" class="redText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" redText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" redText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" redText" />
									<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
										class=" redText" />
								</VBox>
							</HBox>
						</VBox>

						<VBox id="boxResolutionEfficiency" width="10%" alignItems="Center" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>ResolutionEfficiency}"
								design="Bold" class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<VBox width="90%">
								<micro:ComparisonMicroChart size="L"
									width="100%" colorPalette="#74d2e7, #fe5000, #fe5000">
									<micro:data>
										<micro:ComparisonMicroChartData
											title="{StationTrackerI18n>PromisedInTakt}" value="8" />
										<micro:ComparisonMicroChartData
											title="{StationTrackerI18n>PromisedOutTakt}" value="3" />
										<micro:ComparisonMicroChartData
											title="{StationTrackerI18n>NotAcknowledged}" value="10" />
									</micro:data>
								</micro:ComparisonMicroChart>
							</VBox>
						</VBox>

						<VBox id="boxExtraWork" wrap="Wrap" width="5%" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>ExtraWork}" design="Bold"
								width="100%" class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<Label text="{StationTrackerI18n>Operations}" width="100%"
								class="whiteText " />
							<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
								class="whiteText kpiValue" design="Bold" />
							<Label text="{StationTrackerI18n>Hrs}" width="100%" class="whiteText " />
							<Label text="{KPI>/Rowsets/Rowset/0/Row/DisruptionStatus/Total}"
								class="whiteText kpiValue" design="Bold"/>
						</VBox>

						<VBox id="boxShiftStaffing" wrap="Wrap" width="10%" alignItems="Center" class="contentHeaderCont">
							<Label text="{StationTrackerI18n>ShiftSTAFFING}" size="L"
								design="Bold" width="100%" class="whiteTextHeader sapUiSmallMarginTopBottom" />
							<VBox width="90%">
								<micro:DeltaMicroChart width="100%"
									isResponsive="true" value1="10" value2="9"
									title1="{StationTrackerI18n>NeedFTE}" title2="{StationTrackerI18n>AllocatedFTE}"
									displayValue1="100 IM" displayValue2="900 IM"
									deltaDisplayValue="1" />
							</VBox>
						</VBox>
					</HBox>
