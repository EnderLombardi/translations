<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">
	<Dialog title="Select Prod Group" class="sapUiPopupWithPadding"
		id="dialogProdGroup" verticalScrolling="false" horizontalScrolling="false">
		<buttons>
		<Button text="{StationTrackerI18n>WorklistButtonImport}" type="Transparent" icon="sap-icon://forward"
				class="sapUiTinyMarginBeginEnd pageHeaderDialog" press="onOswImport">
			</Button>
			<Button text="{StationTrackerI18n>Close}" icon="sap-icon://decline" press="onCloseDialog" />
		</buttons>
		<content>

			<Select id="selectProdGroup" width="100%" change="selectdDialogProdGroup" items="{ path : 'productionGroupModel>/Rowsets/Rowset/0/Row' }">
				<core:Item key="{productionGroupModel>PROD_GROUP}" text="{productionGroupModel>PROD_GROUP}" />
			</Select>

		</content>
	</Dialog>
</core:FragmentDefinition>
