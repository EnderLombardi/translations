<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">

    <Dialog id="rescheduleAllPopUp" class=""
    		title="{ parts : ['StationTrackerI18n>RescheduleAllPopUpTitle'] }" 
            state="Warning" type="Message"
            stretchOnPhone="true" verticalScrolling="false">
        
        <content>
        	<Text text="{ parts : ['StationTrackerI18n>RescheduleAllPopUpContent', 'RescheduleAllData>/count' ], formatter: 'jQuery.sap.formatMessage' }" />
        </content>

        <buttons>
            <Button id="btnConfirmRescheduleAllPopUp" class="" text="Yes" icon="sap-icon://accept"  press="confirmRescheduleAllPopUp"/>
            <Button id="btnCloseRescheduleAllPopUp"   class="" text="No"  icon="sap-icon://decline" press="closeRescheduleAllPopUp"/>
        </buttons>
        
    </Dialog>

</core:FragmentDefinition>
