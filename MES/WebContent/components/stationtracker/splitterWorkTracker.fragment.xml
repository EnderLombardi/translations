<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified" xmlns:table="sap.ui.table">

			<Table 	id="splitWorTra" 
 					class="splitWorTra" 
 					items="{path: 'SplitDetailModel>/Rowsets/Rowset/0/Row'}" >
					
			<columns>
				<Column>
					<Text text="Start - End Time" />
				</Column>
				<Column>
					<Text text="Operation description" />
				</Column>
				<Column>
					<Text text="Work order description" />
				</Column>
				<Column>
					<Text text="Work center" />
				</Column>
				<Column>
					<Text text="Competency" />
				</Column>
				<Column>
					<Text text="Physical station " />
				</Column>
			</columns>

			<items>
				<ColumnListItem>
					<cells>
						<Text key="{SplitDetailModel>StartEnd}" text="{SplitDetailModel>StartEnd}" />
						<Text key="{SplitDetailModel>Odescription}" text="{SplitDetailModel>Odescription}" />
						<Text key="{SplitDetailModel>WOdescription}" text="{SplitDetailModel>WOdescription}" />
						<Text key="{SplitDetailModel>Wcenter}" 		text="{SplitDetailModel>Wcenter}" />
						<Text key="{SplitDetailModel>Competency}" 	text="{SplitDetailModel>Competency}" />
						<Text key="{SplitDetailModel>Pstation}" 	text="{SplitDetailModel>Pstation}" />
						
					</cells>
				</ColumnListItem>
			</items>
		</Table>
</core:FragmentDefinition>