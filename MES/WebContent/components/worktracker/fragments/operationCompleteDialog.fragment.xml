<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="Full Confirmation" type="Message">
		<content>
			<Text text="{i18n>fullConfirmation}" />
		</content>
		<beginButton>
			<Button text="Cancel" press="onCancelFullConfirmation"/>
		</beginButton>
		<endButton>
			<Button type="Accept" text="OK" press="onFullConfirmation"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>