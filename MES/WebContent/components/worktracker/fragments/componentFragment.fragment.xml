<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:table="sap.ui.table">


	<table:Table  selectionMode="MultiToggle"
		visibleRowCount="5" title="" fixedColumnCount="4" rows="{tableModel>/rows}">
		<table:toolbar>
			<Toolbar>
				<DatePicker value="2014-09-20" valueFormat="yyyy-MM-dd"
					displayFormat="long" change="handleChange" class="sapUiSmallMarginBottom" />
			</Toolbar>
			</table:toolbar>
			<table:toolbar>
			<Toolbar>
				
						<Title text="Components" level="H5" titleStyle="H5" />
						<ToolbarSpacer />
						<Select selectedKey="OrderBy">
							<core:Item key="orderBy" text="Order By" />
						</Select>
						<Select selectedKey="groupBy">
							<core:Item key="groupBy" text="group By" />
						</Select>
					
			</Toolbar>
		</table:toolbar>
		<table:columns>
			<table:Column width="11rem">
				<Label text="" />
				<table:template>
					<Label text="{tableModel>col1}" />
				</table:template>
			</table:Column>
			<table:Column width="11rem">
				<Label text="" />
				<table:template>
					<Label text="{tableModel>col2}" />
				</table:template>
			</table:Column>
			<table:Column width="11rem">
				<Label text="" />
				<table:template>
					<Label text="{tableModel>col3}" />
				</table:template>
			</table:Column>
			<table:Column width="11rem">
				<Label text="" />
				<table:template>
					<Label text="{tableModel>col4}" />
				</table:template>
			</table:Column>
			<table:Column width="11rem">
				<Label text="" />
				<table:template>
					<Label text="{tableModel>col5}" />
				</table:template>
			</table:Column>
		</table:columns>
	</table:Table>
	</core:FragmentDefinition>

