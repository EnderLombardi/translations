<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="Partial Confirmation" type="Message">
		<content>
			<Text text="{i18n>partialConfirmation}" />
		</content>
		<beginButton>
			<Button text="Cancel" press="onCancelConfirmation"/>
		</beginButton>
		<endButton>
			<Button type="Accept" text="OK" press="onPartialConfirmation"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>