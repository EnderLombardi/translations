<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified">
	<Dialog id="confirmDeleteDialog" title="Delete Column"
		stretchOnPhone="true" state="Warning" type="Message">
		<beginButton>
			<Button text="Yes" press="onConfirmDelete" />
		</beginButton>
		<endButton>
			<Button text="No" press="onCancelDelete" type="Reject"/>
		</endButton>
		<content>
			<Text text="Do you want to delete this column ?" />
		</content>
	</Dialog>

</core:FragmentDefinition>