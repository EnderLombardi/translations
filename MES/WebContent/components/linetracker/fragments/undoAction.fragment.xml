<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog id="undoAction" title="Warning"	type="Message" state="Warning">
		<content>
			<Label id="undoText" text="{i18n>undoActionText}"></Label>
		</content>
		<buttons>
			<Button id="idUndoConfirm" text="{i18n>confirm}" press="onUndoConfirm" />
			<Button text="{i18n>cancel}" press="onCancel" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>