<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog id="errorMessageDialog" title="{i18nModel>ErrorMessage}" icon="sap-icon://error" type="Message"
		afterClose="afterMessageDialogClose">
		<buttons>
			<Button text="Close" icon="sap-icon://decline" press="afterMessageDialogClose" />
		</buttons>
		<content>
			<VBox id="messageBox" renderType="List">
			
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>