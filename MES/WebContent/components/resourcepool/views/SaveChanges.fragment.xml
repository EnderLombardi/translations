<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="{i18nModel>SaveChangesTitle}" type="Message">
		<content>
			<Text text="{i18nModel>ConfirmSaveChanges}" />
		</content>
		<buttons>
			<Button text="{i18nModel>Yes}" press="saveChangesUsingDialog" />
			<Button text="{i18nModel>No}" press="afterDialogClose" />
			<Button text="{i18nModel>Cancel}" press="afterDialogCancel" />
		</buttons>

	</Dialog>
</core:FragmentDefinition>