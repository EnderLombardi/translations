<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="{i18nModel>DeleteResourcePoolTitle}" type="Message" icon="sap-icon://warning2">
		<content>
			<VBox>
				<Text text="{i18nModel>ConfirmDelete}" />
				<Text text="" />
				<Text text="{i18nModel>Note}" />
				<Text text="{i18nModel>WarningDelete1}" />
				<Text text="{i18nModel>WarningDelete2}" />
				<Text text="{i18nModel>WarningDelete3}" />
			</VBox>
		</content>
		<buttons>
			<Button text="{i18nModel>OK}" press="deleteResourceUsingDialog" />
			<Button text="{i18nModel>Cancel}" press="afterDeleteDialogCancel" />
		</buttons>

	</Dialog>
</core:FragmentDefinition>