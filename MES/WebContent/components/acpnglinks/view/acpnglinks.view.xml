<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="airbus.mes.acpnglinks.controller.acpnglinks"
	xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:tab="sap.ui.table">
	<Page showHeader="false" height="100%" enableScrolling="false">
		<content>

			<VBox id="ACPnGLinks" class="ACPnGLinkslist">
				<tab:TreeTable id="ACPnGTreeTable" class="ACPnGTreeTable"
					rows="{acpnglinksWorkOrderDetail>/Rowsets/Rowset/0/Row/}"
					cellClick="OnSelectionChange"
					visibleRowCountMode="Fixed" selectionMode="Single" height="100%" >
					<tab:columns>
						<tab:Column visible="true" sortProperty="Type">
							<Label class="ACPnGtLabel" text="Type" />
							<tab:template>
								<Text text="{acpnglinksWorkOrderDetail>Type}" class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="true" sortProperty="Ref">
							<Label class="ACPnGtLabel" text="Reference" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>Ref}" class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="true" sortProperty="Review_end">
							<Label class="ACPnGtLabel" text="ACPnG review end date" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>Review_end}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="true" sortProperty="Note">
							<Label class="ACPnGtLabel" text="Note" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>Note}" class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="true" sortProperty="Family_target">
							<Label class="ACPnGtLabel" text="Family target" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>Family_target}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="STV" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="ACP workstation" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="CA" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="Father link" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="Blocking reason" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="User status" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="ACPnG execution station" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="Origin workstation" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="TDL" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="Upper family" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="Zoning" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="Material description" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
						<tab:Column visible="false">
							<Label text="ATA/SUB-ATA" />
							<tab:template>
								<Label text="{acpnglinksWorkOrderDetail>description}"
									class="AllignItem" />
							</tab:template>
						</tab:Column>
					</tab:columns>
				</tab:TreeTable>
			</VBox>
		</content>
	</Page>
</mvc:View>

